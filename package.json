{
  "name": "mds-test-e2e-",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "e2e": "playwright test",
    "e2e:open": "playwright test --ui",
    "e2e:codegen": "playwright codegen",
    "test-main": "jest test-unitaire/main.spec.ts",
    "test-other": "jest test-unitaire/other.spec.ts",
    "test": "concurrently \"npm run test-main\" \"npm run test-other\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@playwright/test": "^1.41.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "main": "index.js",
  "repository": "https://github.com/JulienNirxo/mds-test-e2e.git",
  "author": "JulienNirxo <millaud.julien@gmail.com>",
  "license": "MIT"
}
